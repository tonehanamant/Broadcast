<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="configBuilders" type="System.Configuration.ConfigurationBuildersSection, System.Configuration, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" restartOnExternalChanges="false" requirePermission="false" />
	<section name="Microsoft.Configuration.ConfigurationBuilders.SectionHandlers" type="Microsoft.Configuration.ConfigurationBuilders.SectionHandlersSection, Microsoft.Configuration.ConfigurationBuilders.Base" restartOnExternalChanges="false" requirePermission="false" />
  </configSections>
  <configBuilders>
    <builders>
        <add name="Secrets" 
		     userSecretsId="a4214d49-a6de-46c7-b793-52d35250c64d" 
			 optional="true"
		     type="Microsoft.Configuration.ConfigurationBuilders.UserSecretsConfigBuilder, Microsoft.Configuration.ConfigurationBuilders.UserSecrets, Version=2.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" 
		     />
	</builders>
  </configBuilders>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <log4net>
    <appender name="CadentServiceAppender" type="Cadent.Library.Logging.Standard.Common.Appenders.CadentLogServiceAppender, CadentLoggingStdCommon" />
    <appender name="EventLogAppender" type="log4net.Appender.EventLogAppender">
      <logName value="Maestro" />
      <applicationName value="BroadcastJobScheduler" />
      <!--<layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level [%thread] %logger - %message%newline" />
      </layout>-->
      <!--json formatted log4net logging-->
      <layout type="log4net.Layout.SerializedLayout, log4net.Ext.Json">
        <decorator type="log4net.Layout.Decorators.StandardTypesDecorator, log4net.Ext.Json" />
        <member value="date:date" />
        <member value="level:level" />
        <member value="thread:thread" />
        <member value="logger:logger" />
        <member value="message:messageObject" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="CadentServiceAppender" />
      <appender-ref ref="EventLogAppender" />
    </root>
  </log4net>
  <Microsoft.Configuration.ConfigurationBuilders.SectionHandlers>
	<handlers>
		<add name="DefaultAppSettingsHandler" type="Microsoft.Configuration.ConfigurationBuilders.AppSettingsSectionHandler" />
		<add name="DefaultConnectionStringsHandler" type="Microsoft.Configuration.ConfigurationBuilders.ConnectionStringsSectionHandler" />
	</handlers>
  </Microsoft.Configuration.ConfigurationBuilders.SectionHandlers>
  <appSettings file="ServiceCommonAppSettings.config" configBuilders="Secrets">
    <!-- Background Job Management Begin -->
    <add key="HangfireDefaultRetryCount" value="2" />
    <add key="HangfireAdminUsername" value="hangfireuser" />
    <add key="HangfireAdminPassword" value="hangfire" />
    <!-- Queue Monitors -->
    <add key="HangfirePollingInterval" value="1000" />
    <add key="HangfireQuickWorkerCountPerProcessor" value="1" />
    <add key="HangfireLongWorkerCountPerProcessor" value="0.5" />
    <!-- Scheduled Jobs -->
    <add key="PlanAutomaticStatusTransitionJobRunHour" value="0" />
    <add key="StationImportJobRunHour" value="0" />
    <add key="ReelIsciiImportJobRunHour" value="1" />
    <!-- Background Job Management End -->
    <!-- Broadcast Logging Application Settings Start -->
    <add key="AppName" value="BroadcastJobScheduler.Service" />
    <add key="ApplicationId" value="275CB563-5FE4-4042-A333-7E345905D05C" />
    <add key="LoggingLogFilePath" value="C:\ct\logs" />
    <add key="MaxFileSizeMb" value="100" />
    <!-- Broadcast Logging Application Settings End -->
  </appSettings>
