{
  "presets": [
    ["env", { 
      "modules": false,
      "targets": {
        "browsers": [
          "Chrome >= 52",
          "FireFox >= 44",
          "Safari >= 7",
          "Explorer 11",
          "last 4 Edge versions"
        ]
      },
      "useBuiltIns": true
      }],
    "react",
  ],
  "plugins": [
    "add-module-exports",
    "transform-object-rest-spread",
    "react-hot-loader/babel",
    "transform-es3-property-literals",
    "transform-es3-member-expression-literals",
  ],
  "env": {
    "development": {
      "plugins": ["istanbul"]
    },
    "production": {
      "presets": [
        ["latest"]
      ]
    }
  }
}
