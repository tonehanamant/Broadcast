<div class="modal fade" tabindex="-1" role="dialog" id="station_edit_rate_thirdparty_modal">
    <div class="modal-dialog modal-med-xl" role="document">
        <div class="modal-content" id="station_edit_rate_thirdparty_view">
            <form id="edit_program_form_thirdparty" action="#">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Edit Program</h4>
                </div>
                <div class="modal-body modal-body-lg">
                    <div class="row">
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Program Name</label>
                                <input id="edit_program_name_thirdparty_input" type="text" class="form-control" disabled>
                            </div>
                        </div>

                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Air Time</label>
                                <input id="edit_program_airtime_thirdparty_input" type="text" class="form-control" disabled>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Genre</label>
                                <select multiple id="edit_program_genre_thirdparty_input" name="edit_program_genre_thirdparty_input" class="form-control select2"></select>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Flight</label>
                                <input id="edit_program_flight_thirdparty_input" type="text" class="form-control" disabled>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Effective Date</label>
                                <input id="edit_program_effective_thirdparty_input" name="edit_program_effective_thirdparty_input" type="text" class="form-control read-only-picker">
                            </div>
                            <div class="help-block" style="margin-top: -10px; font-size: 10px;">Effective Date set to Monday of each Week</div>
                        </div>
                    </div>
                </div>
                <div id="rate_demo_thirdparty_grid" style="height: 300px; margin: 0 15px 20px 15px;"></div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    <button id="edit_program_thirdparty_save_btn" type="button" class="btn btn-success">Save</button>
                </div>
            </form>
        </div>
    </div>
</div>

