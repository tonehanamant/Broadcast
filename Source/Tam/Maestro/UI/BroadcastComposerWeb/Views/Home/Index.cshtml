
@{
    ViewBag.Title = "Traffic";
}
<!-- Begin Tracker Views (Body Tracker specific context from _Layout)-->
@RenderPage("TrackerView.cshtml")
@RenderPage("ScheduleModal.cshtml")
@RenderPage("ScrubModal.cshtml")
@RenderPage("BvsFileListingModal.cshtml")

@section scripts {
    @Scripts.Render("~/bundles/tracker")
    <script>
        var pageController = new TrackerMainController();
        var appController = new AppController();
        $(document).ready(function () {
            //initialize tracker controller
            (function () {
                appController.initControllerPage(pageController);
            })();
        });
    </script>
}