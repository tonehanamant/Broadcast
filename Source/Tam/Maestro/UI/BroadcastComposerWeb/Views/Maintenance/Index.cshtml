
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>

    <style>
        body {
            font-family: Arial;
            padding: 0;
            margin: 0;
        }
        /* Style the tab */
        .tab {
            position: fixed;
            overflow: hidden;
            border: 1px solid #ccc;
            background-color: #eceeef;
            width: 100%;
            z-index: 100;
            top: 0;
        }

            /* Style the buttons inside the tab */
            .tab button {
                background-color: inherit;
                float: left;
                border: none;
                outline: none;
                cursor: pointer;
                padding: 14px 16px;
                transition: 0.3s;
                font-size: 18px;
                color: #41515f;
            }

                /* Change background color of buttons on hover */
                .tab button:hover {
                    background-color: #b3c0cb;
                }

                /* Create an active/current tablink class */
                .tab button.active {
                    background-color: #b3c0cb;
                }

        /* Style the tab content */
        .tabcontent {
            display: none;
            padding: 6px 12px;
            margin-top: 75px;
        }

        .input-container {
            display: flex;
            flex-direction: column;
            max-width: 800px;
        }

        .input-container-row {
            display: flex;
            flex-direction: row;
            justify-content: space-between;
            flex-basis: auto;
            max-width: 800px;
        }

            .input-container-row > *:first-child {
                width: 60%;
            }

            .input-container-row > *:last-child {
                width: 35%;
            }

        .input {
            box-sizing: border-box;
            margin: 0;
            padding: 0;
            border: 0;
            font-weight: 500;
            font-size: 14px;
            font-family: Arial;
            line-height: 24px;
            color: #41515f;
            background: #fff;
            padding-top: 3px !important;
            padding-bottom: 3px !important;
            padding-right: 3px;
            padding-left: 3px;
            border-radius: 4px;
            border: 1px solid #b3c0cb;
            transition: border 0.15s ease-in-out;
            width: inherit;
            text-overflow: ellipsis;
            max-width: 800px;
        }

        .label {
            display: inline-block;
            font-weight: 600;
            font-size: 16px;
            font-family: Arial;
            line-height: 24px;
            color: #688197;
            margin-bottom: 4px;
        }

        .button {
            position: relative;
            background: #41515f;
            border: 1px solid #41515f;
            text-align: center;
            transition: all ease-in-out 0.2s;
            min-width: 60px;
            cursor: pointer;
            border-radius: 4px;
            padding: 7px 23px;
            color: #fff;
            font-weight: 500;
            font-size: 14px;
            font-family: Arial;
            line-height: 24px;
            max-width: 800px;
        }

        .form {
            padding: 5px 30px;
            display: flex;
            flex-direction: column;
            border-bottom: 1px solid #b3c0cb;
        }

            .form:last-child {
                border-bottom: none;
            }

        .inner-form {
            display: flex;
            flex-direction: column;
        }

            .inner-form > * {
                margin-bottom: 10px;
                width: 100%;
            }

        .form > * {
            margin-bottom: 10px;
            width: 100%;
        }

        .heading {
            font-weight: 600;
            font-size: 24px;
            font-family: Arial;
            color: #41515f;
        }

        .heading2 {
            font-weight: 600;
            font-size: 18px;
            font-family: Arial;
            color: #41515f;
        }

        .heading4 {
            font-weight: 600;
            font-size: 16px;
            font-family: Arial;
            color: #41515f;
        }

        .message {
            font-weight: 600;
            font-size: 16px;
            font-family: Arial;
            color: #41515f;
        }

        .text-bar {
            position: fixed;
            background-color: #eceeef;
            display: flex;
            padding: 8px 16px;
            width: 100%;
            border: 1px solid transparent;
            border-color: #41515f;
            top: 50px;
            font-weight: 500;
            font-size: 16px;
            font-family: Arial;
            color: #41515f;
        }
    </style>
</head>
<body>

    <div class="tab">
        <button class="tablinks" id="inventory-tab" onclick="openTab('inventory')">Inventory</button>
        <button class="tablinks" id="program_guide-tab" onclick="openTab('program_guide')">Program Guide</button>
        <button class="tablinks" id="planning-tab" onclick="openTab('planning')">Plannning</button>
        <button class="tablinks" id="buying-tab" onclick="openTab('buying')">Buying</button>
        <button class="tablinks" id="posting-tab" onclick="openTab('posting')">Posting</button>
        <button class="tablinks" id="spot_exceptions_sync-tab" onclick="openTab('spot_exceptions_sync')">Spot Exceptions Sync</button>
        <button class="tablinks" id="reference_data-tab" onclick="openTab('reference_data')">Reference Data</button>
        <button class="tablinks" id="settings-tab" onclick="openTab('settings')">Status & Configuration</button>
        <button class="tablinks" id="data-maintenance-tab" onclick="openTab('data_maintenance')">Data Maintenance</button>
        <button class="tablinks" id="aab-maintenance-tab" onclick="openTab('aab_maintenance')">AAb Maintenance</button>
        <button class="tablinks" id="migrations-tab" onclick="openTab('migrations')">Migrations</button>
    </div>
    @if (ViewBag.Message != null)
    {
        <div id="text-bar" class="text-bar">
            @ViewBag.Message
        </div>
    }
<content>
    <div id="inventory" class="tabcontent">
        <div class="form">
            <h1 class="heading">Jobs</h1>
            @using (Html.BeginForm("RequeueInventoryRatingsJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Requeue Ratings Job" id="submit" />
                </div>
            }

            @using (Html.BeginForm("RequeueInventoryRatingsJobAndWait", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Re-run Ratings Job and wait" id="submit" />
                </div>
            }

            @using (Html.BeginForm("ResetRatingsJobStatus", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Reset Ratings Job Status" id="submit" />
                </div>

            }
            @using (Html.BeginForm("QueueInventorySourceAggregation", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="input-container-row">
                    <input class="input" type="text" name="inventorySourceId" />
                    <input class="button" type="submit" value="Queue Inventory Source Aggregation" id="submit" />
                </div>

            }
            @using (Html.BeginForm("RequeueScxGenerationJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Requeue SCX Generation Job" id="submit" />
                </div>

            }

            @using (Html.BeginForm("ResetScxGenerationJobStatus", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Reset SCX Generation Job Status" id="submit" />
                </div>

            }
        </div>
    </div>
    <div id="program_guide" class="tabcontent">
        <div class="form">
            <h1 class="heading">Inventory Programs Processing</h1>
            <div class="inner-form">
                <h2 class="heading2">By File</h2>
                @using (Html.BeginForm("QueInventoryProgramsJobByFileName", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="inner-form">
                        <h4 class="heading4">Que Inventory Programs Job By File Name - By File Name</h4>
                        <div class="input-container">
                            <label class="label" for="fileName">FileName:</label>
                            <input class="input" type="text" name="fileName" />
                        </div>
                        <input class="button" type="submit" value="Que Inventory Programs Job By File Name" id="submit" />
                    </div>
                }
                @using (Html.BeginForm("QueueInventoryProgramsFileJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="inner-form">
                        <h4 class="heading4">Queue Inventory Programs File Job - By File Id</h4>
                        <div class="input-container">
                            <label class="label" for="fileId">FileId:</label>
                            <input class="input" type="text" name="fileId" />
                        </div>
                        <input class="button" type="submit" value="Queue Inventory Programs File Job" id="submit" />
                    </div>
                }
                @using (Html.BeginForm("RequeueInventoryProgramsByFileJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="inner-form">
                        <h4 class="heading4">Requeue Inventory Programs By File Job - By Job Id</h4>
                        <div class="input-container">
                            <label class="label" for="jobId">JobId:</label>
                            <input class="input" type="text" name="jobId" />
                        </div>
                        <input class="button" type="submit" value="Requeue Inventory Programs By File Job" id="submit" />
                    </div>
                }

            </div>
            <div class="inner-form">
                <h2 class="heading2">By Source</h2>
                @using (Html.BeginForm("QueueInventoryProgramsSourceJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="inner-form">
                        <h4 class="heading4">Queue Inventory Programs Source Job - By Source and Date Range</h4>
                        <div class="input-container">
                            <label class="label" for="inventorySourceId">Inventory Source Id:</label>
                            <input class="input" type="text" name="inventorySourceId" />
                        </div>
                        <div class="input-container">
                            <label class="label" for="startDateString">StartDate (yyyy-MM-dd):</label>
                            <input class="input" type="text" name="startDateString" />
                        </div>
                        <div class="input-container">
                            <label class="label" for="endDateString">EndDate (yyyy-MM-dd):</label>
                            <input class="input" type="text" name="endDateString" />
                        </div>
                        <input class="button" type="submit" value="Queue Inventory Programs Source Job" id="submit" />
                    </div>
                }
                @using (Html.BeginForm("RequeueInventoryProgramsSourceJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="inner-form">
                        <h4 class="heading4">Requeue Inventory Programs Source Job - By Job Id</h4>
                        <div class="input-container">
                            <label class="label" for="jobId">JobId:</label>
                            <input class="input" type="text" name="jobId" />
                        </div>
                        <input class="button" type="submit" value="Requeue Inventory Programs Source Job" id="submit" />
                    </div>
                }
                @using (Html.BeginForm("QueueProcessInventoryProgramsBySourceForWeeks", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="inner-form">
                        <h4 class="heading4">Queue Inventory Programs By Source For Weeks - 2 weeks back and 3 weeks forward from the OrientByDate</h4>
                        <div class="input-container">
                            <label class="label" for="orientByDateString">OrientByDate (yyyy-MM-dd):</label>
                            <input class="input" type="text" name="orientByDateString" />
                        </div>
                        <input class="button" type="submit" value="Queue Inventory Programs By Source For Weeks" id="submit" />
                    </div>
                }

                @using (Html.BeginForm("QueueProcessInventoryProgramsBySourceJobUnprocessedBetweenDates", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="inner-form">
                        <h4 class="heading4">Queue Inventory Programs By Source Job Unprocessed Between Dates - By Source and Date Range where the inventory is not yet processed.</h4>
                        <div class="input-container">
                            <label class="label" for="inventorySourceId">Inventory Source Id :</label>
                            <input class="input" type="text" name="inventorySourceId" />
                        </div>
                        <div class="input-container">
                            <label class="label" for="startDateString">StartDate (yyyy-MM-dd):</label>
                            <input class="input" type="text" name="startDateString" />
                        </div>
                        <div class="input-container">
                            <label class="label" for="endDateString">EndDate (yyyy-MM-dd):</label>
                            <input class="input" type="text" name="endDateString" />
                        </div>
                        <input class="button" type="submit" value="Queue Inventory Programs By Source Job Unprocessed Between Dates" id="submit" />
                    </div>
                }
            </div>
        </div>
    </div>
    <div id="planning" class="tabcontent">
        @using (Html.BeginForm("RerunPlanPricingJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Rerun Plan Pricing Job</h4>
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Rerun Plan Pricing Job" id="submit" />
                </div>
            </div>
        }

        @using (Html.BeginForm("ForceCompletePlanPricingJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Force Complete Plan Pricing Job</h4>
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Force complete a Plan Pricing Job" id="submit" />
                </div>
            </div>
        }

        @using (Html.BeginForm("ComparePlanVersionForShouldPromotePricing", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Compare Plan Version For "Should Promote Pricing"</h4>
                <div class="input-container-row">
                    <div class="input-container">
                        <label class="label" for="planId">PlanId:</label>
                        <input class="input" type="text" name="planId" />
                    </div>
                </div>
                <div class="input-container-row">
                    <div class="input-container">
                        <label class="label" for="beforeVersionId">Before Plan Version Id:</label>
                        <input class="input" type="text" name="beforeVersionId" />
                    </div>
                </div>
                <div class="input-container-row">
                    <div class="input-container">
                        <label class="label" for="afterVersionId">After Plan Version Id:</label>
                        <input class="input" type="text" name="afterVersionId" />
                    </div>
                </div>
                <input class="button" type="submit" value="Compare" id="submit" />
            </div>
        }

        @using (Html.BeginForm())
        {
            <div class="form">
                <h4 class="heading">Generate Pricing Results Report</h4>
                <div class="input-container">
                    <label class="label" for="planId">Plan Id</label>
                    <input class="input" type="text" name="planId" placeholder="e.g. '17'" />
                </div>
                <div class="btn-group">
                    <input class="btn button" type="submit" value="Generate Quality" id="btnQuality" formaction="GeneratePricingResultsReportQuality" formmethod="post" />
                    <input class="btn button" type="submit" value="Generate Efficiency" id="btnEfficiency" formaction="GeneratePricingResultsReportEfficiency" formmethod="post" />
                    <input class="btn button" type="submit" value="Generate Floor" id="btnFloor" formaction="GeneratePricingResultsReportFloor" formmethod="post" />
                </div>
            </div>
        }

        @using (Html.BeginForm("TestPricingRequestLogBucket", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Test Pricing Request Log Process (S3 Access)</h4>
                This triggers an aysnc thread that places a file in the bucket as ApiVersion = 789.
                <br />Errors will not be reported to this screen.
                <input class="button" type="submit" value="Trigger Test" id="submit" />
            </div>
        }
    </div>

    <div id="buying" class="tabcontent">

        @using (Html.BeginForm("RerunPlanBuyingJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Rerun Plan Buying Job</h4>
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Rerun Plan Buying Job" id="submit" />
                </div>
            </div>
        }

        @using (Html.BeginForm("ForceCompletePlanBuyingJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Force Complete Plan Buying Job</h4>
                <div class="input-container-row">
                    <input class="input" type="text" name="jobId" />
                    <input class="button" type="submit" value="Force complete a Plan Buying Job" id="submit" />
                </div>
            </div>
        }

        @using (Html.BeginForm("GenerateBuyingResultsReportEfficiency", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Generate Buying Results Report - Efficiency</h4>
                <div class="input-container">
                    <label class="label" for="planId">Plan Id</label>
                    <input class="input" type="text" name="planId" placeholder="e.g. '17'" />
                </div>
                <input class="button" type="submit" value="Generate" id="submit" />
            </div>
        }

        @using (Html.BeginForm("GenerateBuyingResultsReportFloor", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h4 class="heading">Generate Buying Results Report - Floor</h4>
                <div class="input-container">
                    <label class="label" for="planId">Plan Id</label>
                    <input class="input" type="text" name="planId" placeholder="e.g. '17'" />
                </div>
                <input class="button" type="submit" value="Generate" id="submit" />
            </div>
        }


    </div>

    <div id="posting" class="tabcontent">
        <form class="form" action="" method="post" enctype="multipart/form-data">
            <h1 class="heading">Processing affidavit files</h1>
            <div class="input-container">
                <label class="label" for="file">Filename:</label>
                <input class="input" type="file" name="file" id="file" />
            </div>
            <input class="button" type="submit" text="generate JSON" />
            <a href="javascript:copyToClip();">Copy to Clipboard</a>
            @{
                var IdValue = "";
                if (ViewBag.Id != null)
                {
                    IdValue = "Uploaded AffidavidID = " + ViewBag.Id + "";
                }
            }
            @IdValue
            <div class="input-container">
                <div class="label">Results:</div>
                <textarea class="input" rows="5" id="divJSON">@ViewBag.TextareaMessage</textarea>
            </div>
        </form>
    </div>
    <div id="reference_data" class="tabcontent">
        @using (Html.BeginForm("UploadNTIUniversesFile", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h1 class="heading">Upload NTI Universes</h1>
                <div class="input-container">
                    <label class="label" for="file">Filename:</label>
                    <input class="input" type="file" name="file" id="file" />
                </div>
                <input class="button" type="submit" text="Upload NTI Universes" />
                <div>
                    <span class="label">Result:</span>
                    <label class="label" id="result_nti_universes">@ViewBag.Message</label>
                </div>
            </div>
        }
        @using (Html.BeginForm("UploadMarketCoverageFile", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h1 class="heading">Upload Market Coverages</h1>
                <div class="input-container">
                    <label class="label" for="file">Filename:</label>
                    <input class="input" type="file" name="file" id="file" />
                </div>
                <input class="button" type="submit" text="Upload Market Coverages" />
                <div>
                    <span class="label">Result:</span>
                    <label class="label" id="result_market_coverages">@ViewBag.Message</label>
                </div>
            </div>

        }

        @using (Html.BeginForm("ImportStationMappings", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h1 class="heading">Import station mappings</h1>
                <div class="input-container">
                    <label class="label" for="file">Filename:</label>
                    <input class="input" type="file" name="file" id="file" />
                </div>
                <input class="button" type="submit" value="Submit" id="txt Import Station Mappings" />
                <textarea class="input" rows="5" id="divJSON">@ViewBag.Message</textarea>
            </div>
        }

        @using (Html.BeginForm("PerformImportStationsFromForecastDatabase", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h1 class="heading">Perform Nightly Stations Update</h1>
                <input class="button" type="submit" value="Submit" id="txt Perform Nightly Stations Update" />
            </div>
        }

        @using (Html.BeginForm("ResetLastStationsFromForecastDatabaseJob", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form">
                <h1 class="heading">Reset the last Nightly Stations Update</h1>
                <input class="button" type="submit" value="Submit" id="txt Reset the Last Nightly Stations Update" />
            </div>
        }

        <div class="form">
            <h1 class="heading">Cleanup Dayparts</h1>
            @using (Html.BeginForm("CleanupDaypartsFilterErroneousDayparts", "Maintenance", FormMethod.Post))
            {
                <div class="input-container">
                    <input class="button" type="submit" value="Show erroneous dayparts" id="submit" />
                </div>
            }
            @using (Html.BeginForm("CleanupDaypartsRepairErroneousDayparts", "Maintenance", FormMethod.Post))
            {
                <div class="input-container">
                    <input class="button" type="submit" value="Repair erroneous dayparts" id="submit" />
                </div>
            }
            <div class="input-container">
                <span class="label">Results:</span>
                <textarea class="input" rows="5" id="divCleanupJSON">@ViewBag.TextareaMessage</textarea>
            </div>
        </div>
        <div class="form">
            <h1 class="heading">Program Name Mappings</h1>
            @using (Html.BeginForm("ImportProgramMappings", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="form">
                    <h1 class="heading">Import program mappings</h1>
                    Import program mappings into the program mappings table and reprocess inventory to update with new mappings.
                    <div class="input-container">
                        <label class="label" for="file">Filename:</label>
                        <input class="input" type="file" name="file" id="file" />
                    </div>
                    <input class="button" type="submit" value="Submit" id="txt Import Program Mappings" />
                    <textarea class="input" rows="5" id="divJSON">@ViewBag.Message</textarea>
                </div>
            }
            @using (Html.BeginForm("ExportProgramMappings", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="form">
                    <h1 class="heading">Export program mappings</h1>
                    Export the program mappings from the program mappings table.
                    <input class="button" type="submit" value="Submit" id="txt Export Program Mappings" />
                    <textarea class="input" rows="5" id="divJSON">@ViewBag.Message</textarea>
                </div>
            }
            @using (Html.BeginForm("ExportUnmappedProgramNames", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="form">
                    <h1 class="heading">Export unmapped program names</h1>
                    Export all the unmapped program names in the system's inventory.
                    <input class="button" type="submit" value="Submit" id="txt Export-Unmapped-Program-names" />
                    <textarea class="input" rows="1" id="divJSON">@ViewBag.Message</textarea>
                </div>
            }

            @using (Html.BeginForm("ImportMasterList", "Maintenance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
