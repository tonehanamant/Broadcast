    <script>
        function copyToClip() {
            var copyText = document.getElementById("divJSON");
            /* Select the text field */
            copyText.select();
            /* Copy the text inside the text field */
            document.execCommand("Copy");

        };
        var openTab = function (tabId) {
            var i, tabcontent, tablinks;
            tabcontent = document.getElementsByClassName("tabcontent");
            for (i = 0; i < tabcontent.length; i++) {
                tabcontent[i].style.display = "none";
            }
            tablinks = document.getElementsByClassName("tablinks");
            for (i = 0; i < tablinks.length; i++) {
                tablinks[i].className = tablinks[i].className.replace(" active", "");
            };
            tabEl = document.getElementById(tabId);
            tabEl.style.display = "block";
            tabEl.className += " active";
            tabLink = document.getElementById(tabId + "-tab");
            tabLink.className += " active";
        }
        var clickLink = function () {
            var tab = "@ViewBag.TabId"
            if (tab) {
                openTab(tab)
            } else {
                openTab('inventory')
            }

        }
        document.addEventListener("DOMContentLoaded", clickLink);
    </script>

