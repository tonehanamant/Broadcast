    <compilation debug="true" targetFramework="4.7.2">
      <assemblies>
        <add assembly="System.Runtime, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
      </assemblies>
    </compilation>
    <httpRuntime targetFramework="4.7.2" maxRequestLength="1048576" />
    <customErrors mode="Off" />
  </system.web>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Rewrite React Requests" stopProcessing="true">
          <match url="^broadcastreact/(.*)$" />
          <conditions>
            <!-- <add input="{HTTP_ACCEPT}" pattern="^text/html" /> -->
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
          </conditions>
          <action type="Rewrite" url="/broadcastreact/index.html" />
        </rule>
        <rule name="Rewrite Broadcast Requests" stopProcessing="true">
          <match url="^broadcast/(.*)$" />
          <conditions>
            <!-- <add input="{HTTP_ACCEPT}" pattern="^text/html" /> -->
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
          </conditions>
          <action type="Rewrite" url="/{R:1}" />
        </rule>
      </rules>
    </rewrite>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="1073741824" />
      </requestFiltering>
    </security>
  </system.webServer>
